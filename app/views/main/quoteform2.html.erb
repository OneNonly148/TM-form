    <input type="text" id="fv_name" placeholder="Name" class="form"></input><br/><br/>
    <input type="text" id="fv_phone" placeholder="Phone No." class="form"></input><br/><br/>
    <input type="text" id="fv_fax" placeholder="Fax No." class="form"></input><br/><br/>
    <input type="text" id="fv_email" placeholder="Email" class="form"></input><hr/>
